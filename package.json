{
  "name": "webshop",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "eslint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "postinstall": "pnpm manypkg fix"
  },
  "manypkg": {
    "ignoredRules": [
      "INTERNAL_MISMATCH"
    ]
  },
  "packageManager": "pnpm@8.9.0",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@config/eslint-config": "0.0.0",
    "@manypkg/cli": "latest",
    "@repo/typescript-config": "0.0.0",
    "@turbo/gen": "1.10.12",
    "@types/node": "20.10.5",
    "prettier": "3.1.0",
    "turbo": "latest",
    "typescript": "5.3.3"
  }
}